{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["plugin:react/recommended", "standard", "prettier"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "indent": "off"
    }
  }
  